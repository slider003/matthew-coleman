{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth",
      "dest": "/auth.js"
    },
    {
      "src": "/callback",
      "dest": "/callback.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
